{
    "manifest_version": 3,
    "name": "screenshot window",
    "version": "1.0",
    "permissions": ["activeTab", "downloads", "tabs"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ]
  }
  